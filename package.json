    "tslib": "^1.9.3",
    "prop-types": "^15.x",
    "react": "^16.2.x"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/react": "^16.4.12",
    "@types/react-test-renderer": "^16.0.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "mocha-clean": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-test-renderer": "^16.4.2",
